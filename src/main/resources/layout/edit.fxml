<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.edit.EditController">
   <children>
      <HBox>
         <children>
            <Button mnemonicParsing="false" onAction="#back" text="Артка">
               <HBox.margin>
                  <Insets bottom="4.0" left="10.0" right="4.0" top="4.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <SplitPane dividerPositions="0.3, 0.7" VBox.vgrow="ALWAYS">
         <items>
            <AnchorPane fx:id="paneLeft" prefHeight="200.0" prefWidth="200.0">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </AnchorPane>
            <AnchorPane fx:id="paneCenter" prefWidth="185.0" />
            <AnchorPane fx:id="paneRight" />
         </items>
      </SplitPane>
   </children>
</VBox>
